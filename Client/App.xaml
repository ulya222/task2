<Application x:Class="DataVault.Client.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             StartupUri="Screens/LoginWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/DataVault.Client;component/Theme.xaml"/>
            </ResourceDictionary.MergedDictionaries>
            <FontFamily x:Key="AppFont">Consolas, Courier New</FontFamily>
        </ResourceDictionary>
    </Application.Resources>
</Application>
